<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_step_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_horizontal_3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_horizontal_2"
        app:layout_constraintTop_toTopOf="parent"
        tools:targetApi="lollipop" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_step_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_horizontal_4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_horizontal_3"
        app:layout_constraintTop_toTopOf="parent"
        tools:targetApi="lollipop" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_step_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_horizontal_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_horizontal_1"
        app:layout_constraintTop_toTopOf="parent"
        tools:targetApi="lollipop" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton

        android:id="@+id/fab_step_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:background="@color/colorWhite"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_horizontal_1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:targetApi="lollipop" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton

        android:id="@+id/fab_step_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="16dp"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_horizontal_4"
        app:layout_constraintTop_toTopOf="parent"
        tools:targetApi="lollipop" />

    <TextView
        android:id="@+id/tv_horizontal_1"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:adjustViewBounds="true"

        android:background="@color/colorGrey_250"
        app:layout_constraintBottom_toBottomOf="@+id/fab_step_1"
        app:layout_constraintEnd_toStartOf="@+id/fab_step_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/fab_step_1"
        app:layout_constraintTop_toTopOf="@+id/fab_step_1" />

    <TextView
        android:id="@+id/tv_horizontal_4"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:background="@color/colorGrey_250"
        app:layout_constraintBottom_toBottomOf="@+id/fab_step_5"
        app:layout_constraintEnd_toStartOf="@+id/fab_step_5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/fab_step_4"
        app:layout_constraintTop_toTopOf="@+id/fab_step_5" />

    <TextView
        android:id="@+id/tv_horizontal_2"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:background="@color/colorGrey_250"
        app:layout_constraintBottom_toBottomOf="@+id/fab_step_2"
        app:layout_constraintEnd_toStartOf="@+id/fab_step_3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/fab_step_2"
        app:layout_constraintTop_toTopOf="@+id/fab_step_2" />

    <TextView
        android:id="@+id/tv_horizontal_3"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:background="@color/colorGrey_250"
        app:layout_constraintBottom_toBottomOf="@+id/fab_step_4"
        app:layout_constraintEnd_toStartOf="@+id/fab_step_4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/fab_step_3"
        app:layout_constraintTop_toTopOf="@+id/fab_step_4" />

    <investwell.utils.customView.CustomTextViewBold
        android:id="@+id/tv_step_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="1"
        android:textStyle="bold"
        android:translationZ="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/fab_step_1"
        app:layout_constraintEnd_toEndOf="@+id/fab_step_1"
        app:layout_constraintStart_toStartOf="@+id/fab_step_1"
        app:layout_constraintTop_toTopOf="@+id/fab_step_1"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="359dp"
        tools:targetApi="lollipop" />

    <investwell.utils.customView.CustomTextViewBold
        android:id="@+id/tv_step_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="2"
        android:textStyle="bold"
        android:translationZ="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/fab_step_2"
        app:layout_constraintEnd_toEndOf="@+id/fab_step_2"
        app:layout_constraintStart_toStartOf="@+id/fab_step_2"
        app:layout_constraintTop_toTopOf="@+id/fab_step_2"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="359dp"
        tools:targetApi="lollipop" />

    <investwell.utils.customView.CustomTextViewBold
        android:id="@+id/tv_step_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="3"
        android:textStyle="bold"
        android:translationZ="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/fab_step_3"
        app:layout_constraintEnd_toEndOf="@+id/fab_step_3"
        app:layout_constraintStart_toStartOf="@+id/fab_step_3"
        app:layout_constraintTop_toTopOf="@+id/fab_step_3"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="359dp"
        tools:targetApi="lollipop" />

    <investwell.utils.customView.CustomTextViewBold
        android:id="@+id/tv_step_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="4"
        android:textStyle="bold"
        android:translationZ="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/fab_step_4"
        app:layout_constraintEnd_toEndOf="@+id/fab_step_4"
        app:layout_constraintStart_toStartOf="@+id/fab_step_4"
        app:layout_constraintTop_toTopOf="@+id/fab_step_4"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="359dp"
        tools:targetApi="lollipop" />

    <investwell.utils.customView.CustomTextViewBold
        android:id="@+id/tv_step_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="5"
        android:textStyle="bold"
        android:translationZ="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/fab_step_5"
        app:layout_constraintEnd_toEndOf="@+id/fab_step_5"
        app:layout_constraintStart_toStartOf="@+id/fab_step_5"
        app:layout_constraintTop_toTopOf="@+id/fab_step_5"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="359dp"
        tools:targetApi="lollipop" />
</androidx.constraintlayout.widget.ConstraintLayout>






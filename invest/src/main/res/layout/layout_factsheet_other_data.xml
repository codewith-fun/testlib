<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/CardViewStyle"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <investwell.utils.customView.CustomTextViewRegular
            android:id="@+id/textView24"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/factsheet_fund_size_header_title"
            android:textColor="?attr/secondaryTextColor"
            android:textSize="@dimen/textView_all_size_10"
            app:layout_constraintEnd_toStartOf="@+id/textView25"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <investwell.utils.customView.CustomTextViewRegular
            android:id="@+id/textView25"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/factsheet_fund_age_header_title"
            android:textColor="?attr/secondaryTextColor"

            android:textSize="@dimen/textView_all_size_10"
            app:layout_constraintEnd_toStartOf="@+id/textView26"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/textView24"
            app:layout_constraintTop_toTopOf="parent" />

        <investwell.utils.customView.CustomTextViewRegular
            android:id="@+id/textView26"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/factsheet_fund_category_header_title"
            android:textColor="?attr/secondaryTextColor"

            android:textSize="@dimen/textView_all_size_10"
            app:layout_constraintEnd_toStartOf="@+id/textView27"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/textView25"
            app:layout_constraintTop_toTopOf="parent" />

        <investwell.utils.customView.CustomTextViewRegular
            android:id="@+id/textView27"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="@string/factsheet_fund_expense_header_title"
            android:textColor="?attr/secondaryTextColor"

            android:textSize="@dimen/textView_all_size_10"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/textView26"
            app:layout_constraintTop_toTopOf="parent" />

        <investwell.utils.customView.CustomTextViewBold
            android:id="@+id/tv_fund_size"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="@dimen/margin_all_very_small"
            android:textColor="?attr/primaryTextColor"
            android:textSize="@dimen/textView_all_size_12"
            android:textStyle="bold"

            app:layout_constraintBottom_toTopOf="@+id/view3"
            app:layout_constraintEnd_toStartOf="@+id/tv_fund_age"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView24"
            app:layout_constraintVertical_bias="0.0"
            tools:text="@string/app_name" />

        <investwell.utils.customView.CustomTextViewBold
            android:id="@+id/tv_fund_age"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="@dimen/margin_all_very_small"
            android:textColor="?attr/primaryTextColor"
            android:textSize="@dimen/textView_all_size_12"
            android:textStyle="bold"

            app:layout_constraintBottom_toTopOf="@+id/view3"
            app:layout_constraintEnd_toStartOf="@+id/tv_fund_category"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_fund_size"
            app:layout_constraintTop_toBottomOf="@+id/textView25"
            app:layout_constraintVertical_bias="0.0"
            tools:text="@string/app_name" />

        <investwell.utils.customView.CustomTextViewBold
            android:id="@+id/tv_fund_category"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="@dimen/margin_all_very_small"
            android:textColor="?attr/primaryTextColor"
            android:textSize="@dimen/textView_all_size_12"
            android:textStyle="bold"

            app:layout_constraintBottom_toTopOf="@+id/view3"
            app:layout_constraintEnd_toStartOf="@+id/tv_fund_expense"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_fund_age"
            app:layout_constraintTop_toBottomOf="@+id/textView26"
            app:layout_constraintVertical_bias="0.0"
            tools:text="@string/app_name" />

        <investwell.utils.customView.CustomTextViewBold
            android:id="@+id/tv_fund_expense"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="@dimen/margin_all_very_small"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:textColor="?attr/primaryTextColor"
            android:textSize="@dimen/textView_all_size_12"
            android:textStyle="bold"

            app:layout_constraintBottom_toTopOf="@+id/view3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_fund_category"
            app:layout_constraintTop_toBottomOf="@+id/textView27"
            app:layout_constraintVertical_bias="0.0"
            tools:text="@string/app_name" />

        <View
            android:id="@+id/view3"
            android:layout_width="0dp"
            android:layout_height="1dp"
           android:layout_marginTop="@dimen/margin_all_regular"
            android:background="@color/colorGrey_250"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_fund_category" />

        <investwell.utils.customView.CustomTextViewRegular
            android:id="@+id/textView32"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:gravity="start"
            android:text="@string/factsheet_exit_load_header_title"
            android:textColor="?attr/primaryTextColor"
            android:textSize="@dimen/textView_all_size_10"
            app:layout_constrainedWidth="true"
            app:layout_constraintEnd_toStartOf="@+id/tv_launch_date_label"
            app:layout_constraintHorizontal_bias="0.023"
            app:layout_constraintStart_toStartOf="@+id/tv_fund_size"
            app:layout_constraintTop_toBottomOf="@+id/view3" />

        <investwell.utils.customView.CustomTextViewBold
            android:id="@+id/tv_exit_load"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="@dimen/margin_all_small"
            android:paddingBottom="@dimen/padding_all_very_small"
            android:textColor="?attr/primaryTextColor"
app:layout_constrainedWidth="true"
            android:textSize="@dimen/textView_all_size_12"
            android:textStyle="bold"
android:gravity="start"
            app:layout_constraintBottom_toTopOf="@+id/v_divider"
            app:layout_constraintEnd_toStartOf="@+id/tv_launch_date"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView32"
            tools:text="@string/app_name" />

        <investwell.utils.customView.CustomTextViewBold
            android:id="@+id/tv_launch_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="?attr/primaryTextColor"
            android:textSize="@dimen/textView_all_size_12"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/tv_fund_expense"
            app:layout_constraintTop_toBottomOf="@+id/textView32"
            tools:text="26 Aug 2021" />

        <investwell.utils.customView.CustomTextViewRegular
            android:id="@+id/tv_launch_date_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/launch_date"
            android:textColor="?attr/primaryTextColor"
            android:textSize="@dimen/textView_all_size_10"
            app:layout_constraintBottom_toTopOf="@+id/tv_launch_date"
            app:layout_constraintStart_toStartOf="@+id/tv_fund_expense" />
        <View
            android:id="@+id/v_divider"
            android:layout_width="0dp"
            android:layout_height="1dp"

            android:layout_marginTop="8dp"
            android:background="@color/colorGrey_250"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"

            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_exit_load" />

        <investwell.utils.customView.CustomTextViewRegular
            android:id="@+id/textView7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="@string/factsheet_short_term_benchmark_row_title"
            android:textColor="?attr/primaryTextColor"
            android:textSize="@dimen/textView_all_size_10"
            android:visibility="gone"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.023"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/v_divider" />

        <investwell.utils.customView.CustomTextViewBold
            android:id="@+id/tv_benchmark_factsheet"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="@dimen/margin_all_very_small"
            android:textColor="?attr/primaryTextColor"

            android:textSize="@dimen/textView_all_size_14"
            android:textStyle="bold"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintVertical_bias="0.0"
            tools:text="@string/app_name" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>